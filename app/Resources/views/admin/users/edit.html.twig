{% include 'admin/header.html.twig' with {'title': 'Update Profile','extra_css':''}  %}
<body>
<body>
<div id="pcoded" class="pcoded">
        <div class="pcoded-overlay-box"></div>
        <div class="pcoded-container navbar-wrapper">
        {% include 'admin/inner_header.html.twig' %}
		<!-- Sidebar inner chat start-->
                   
        <div class="pcoded-main-container">
                <div class="pcoded-wrapper">
                    <nav class="pcoded-navbar">
                        {% include 'admin/sidebar.html.twig' %}
                    </nav>                                        
        <div class="pcoded-content">
          <div class="pcoded-inner-content">
            <!-- Main-body start -->
            <div class="main-body">
              <div class="page-wrapper">
                <!-- Page-header start -->
                <div class="page-header">
                  <div class="row align-items-end">
                    <div class="col-lg-8">
                      <div class="page-header-title">
                        <div class="d-inline">
                          <h4>Update Profile</h4>
                        </div>
                      </div>
                    </div>
                    <div class="col-lg-4">
                      <div class="page-header-breadcrumb">
                        <ul class="breadcrumb-title">
                          <li class="breadcrumb-item"> <a href="{{ url('admin_dashboard')}}"> <i class="feather icon-home"></i> </a> </li>
                          <li class="breadcrumb-item"><a href="#!">Profile</a> </li>
                          <li class="breadcrumb-item"><a href="#!">Update</a> </li>
                        </ul>
                      </div>
                    </div>
                  </div>
                </div>
                <!-- Page-header end -->
                <!-- Page body start -->
                <div class="page-body">
                  <div class="row">
                    <div class="col-sm-12">
                      <!-- Basic Inputs Validation start -->
                      <div class="card">
                        <div class="card-header"> 
                        {% if user_info.getKycStatus==0 %}
                        <span class="pcoded-badge label label-danger">KYC document approval is pending</span>
                        {% endif %}
                        </div>
                        <div class="card-block">
                          {{ form_start(form, {
                        'attr': {'id': 'update-profile-form'},
                        'action': path('admin_update_user_profile',{user_id: user_id}), 'method': 'POST'}
                        )
               	   }}                    
               			<h5>Check in form</h5> <br>
                         <div class="form-group row">
                           <div class="col-sm-6">
                                <label class="col-form-label">First Name</label>
                                <div class="col-sm-12">
                                {{ form_row(form.first_name) }} 
                                 <span class="messages"></span>
                                 </div>
                            </div>
                           <div class="col-sm-6">
                                <label class="col-form-label">Last Name</label>
                                <div class="col-sm-12">
                                {{ form_row(form.last_name) }} 
                                 <span class="messages"></span>
                                 </div>
                            </div> 
                          </div>
               
             			 <div class="form-group row">
                           <div class="col-sm-6">
                                <label class="col-form-label">Mobile No</label>
                                <div class="col-sm-12">
                                {{ form_row(form.mobile) }}
                                 <span class="messages"></span>
                                 </div>
                            </div>
                           <div class="col-sm-6">
                                <label class="col-form-label">Whatsapp No</label>
                                <div class="col-sm-12">
                                {{ form_row(form.whatsap_no) }} 
                                 <span class="messages"></span>
                                 </div>
                            </div> 
                          </div>                          
                       
                         <div class="form-group row">
                            <div class="col-sm-6">
                                <label class="col-form-label">Choose DOB</label>
                                <div class="col-sm-12">
                                {{ form_row(form.dob) }}
                                 <span class="messages"></span>
                                 </div>
                            </div>
                            
                            <div class="col-sm-6">
                                <label class="col-form-label">Email</label>
                                <div class="col-sm-12">
                               {{ form_row(form.email_id) }} 
                                 <span class="messages"></span>
                                 </div>
                            </div>                          
                            
                          </div>                          
                        
                         <div class="form-group row">
                           
                           <div class="col-sm-6">
                                <label class="col-form-label">Gender</label>
                                <div class="col-sm-12">
                                {{ form_row(form.gender) }}
                                 <span class="messages"></span>
                                 </div>
                            </div>
                           
                           <div class="col-sm-6">
                                <label class="col-form-label">Passport Number</label>
                                <div class="col-sm-12">
                                {{ form_row(form.passport_no) }}
                                 <span class="messages"></span>
                                 </div>
                            </div>
                              
                          </div>
                                                
                         <div class="form-group row">
                             <div class="col-sm-6">
                                <label class="col-form-label">Address</label>
                                <div class="col-sm-12">
                               {{ form_row(form.address) }} 
                                 <span class="messages"></span>
                                 </div>
                            </div>  
                           
                           
                           <div class="col-sm-6">
                                <label class="col-form-label">Photo Id</label>
                                <div class="col-sm-12">
                               {{ form_row(form.kyc_doc_photoid) }}
                                 <span class="messages"></span>
                                 </div>
                            </div>  
                          </div>
      
                        <br> <h5>Emergency Contact</h5> <br> 
                        <div class="form-group row">
                           <div class="col-sm-6">
                                <label class="col-form-label">First Name</label>
                                <div class="col-sm-12">
                                {{ form_row(form.em_first_name) }} 
                                 <span class="messages"></span>
                                 </div>
                            </div>
                           <div class="col-sm-6">
                                <label class="col-form-label">Last Name</label>
                                <div class="col-sm-12">
                                {{ form_row(form.em_last_name) }} 
                                 <span class="messages"></span>
                                 </div>
                            </div> 
                          </div>
                        <div class="form-group row">
                           <div class="col-sm-6">
                                <label class="col-form-label">Emergency Phone</label>
                                <div class="col-sm-12">
                                {{ form_row(form.em_mobile) }} 
                                 <span class="messages"></span>
                                 </div>
                            </div>
                           <div class="col-sm-6">
                                <label class="col-form-label">Emergency Relation</label>
                                <div class="col-sm-12">
                                {{ form_row(form.em_relation) }} 
                                 <span class="messages"></span>
                                 </div>
                            </div> 
                          </div>
                       
                       <br> <h5>Employer/Study Detail</h5> <br> 
                        <div class="form-group row">
                           <div class="col-sm-6">
                                <label class="col-form-label">Employer Name</label>
                                <div class="col-sm-12">
                                {{ form_row(form.employer_name) }} 
                                 <span class="messages"></span>
                                 </div>
                            </div>
                           <div class="col-sm-6">
                                <label class="col-form-label">Work Phone</label>
                                <div class="col-sm-12">
                                {{ form_row(form.work_phone) }} 
                                 <span class="messages"></span>
                                 </div>
                            </div> 
                          </div>
                        <div class="form-group row">
                           <div class="col-sm-6">
                                <label class="col-form-label">Work Email</label>
                                <div class="col-sm-12">
                                {{ form_row(form.work_email) }} 
                                 <span class="messages"></span>
                                 </div>
                            </div>
                         <div class="col-sm-6">
                                <label class="col-form-label">College Name</label>
                                <div class="col-sm-12">
                                {{ form_row(form.college_name) }} 
                                 <span class="messages"></span>
                                 </div>
                            </div>   
                          </div>
                          <div class="form-group row">						    
                            <div class="col-sm-6">
                                <label class="col-form-label">Course</label>
                                <div class="col-sm-12">
                                {{ form_row(form.course) }} 
                                 <span class="messages"></span>
                                 </div>
                            </div> 
                          </div>
                         
                           
                          <div class="form-group row">
                            <label class="col-sm-2"></label>
                            <div class="col-sm-10">
                              {{ form_row(form.Submit) }}	
                            </div>
                          </div>
                          {{ form_end(form) }}
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
                <!-- Page body end -->
              </div>
            </div>
            <!-- Main-body end -->
          </div>
        </div>
            </div>
       </div>          
        </div>
    </div>
 <link rel="stylesheet" type="text/css" href="//code.jquery.com/ui/1.9.2/themes/base/jquery-ui.css">   
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.8.3/underscore-min.js"></script>
 {% include 'admin/footer.html.twig' with {'extra_js':['assets/js/commonjs.js']} %}
